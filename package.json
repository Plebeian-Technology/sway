{
    "name": "@sway/sway",
    "author": {
        "name": "Sway",
        "email": "sway@sway.vote",
        "twitter": "@Sway_Vote"
    },
    "contributors": [
        {
            "name": "Sway",
            "email": "sway@sway.vote",
            "coc-enforcer": true
        }
    ],
    "private": true,
    "workspaces": {
        "packages": [
            "packages/types",
            "packages/constants",
            "packages/utils",
            "packages/fire",
            "packages/webapp",
            "packages/scripts",
            "packages/seeds"
        ]
    },
    "overrides": {
        "autoprefixer": "10.4.5"
    },
    "scripts": {
        "analyze": "pnpm -C ./packages/webapp run analyze",
        "START": "",
        "start": "PORT=3333 pnpm -C ./packages/webapp run start",
        "start:widgets": "pnpm -C ./packages/webapp run start:widgets",
        "start:emulate": "pnpm -C ./packages/webapp run start:emulate",
        "BUILD": "",
        "build": "pnpm -C ./packages/webapp run build",
        "build:widgets": "pnpm -C ./packages/webapp run build:widgets",
        "TEST": "",
        "test": "pnpm -C ./packages/webapp run test",
        "DEPLOY": "",
        "deploy:hosting": "./deploy-hosting.sh",
        "deploy:functions": "firebase deploy --only functions",
        "deploy:function": "firebase deploy --only",
        "LOGS": "",
        "logs:functions": "firebase functions:log",
        "SEEDS": "",
        "seed": "pnpm -C ./packages/seeds run seed:dev",
        "seed:dev": "pnpm -C ./packages/seeds run seed:dev",
        "seed:test": "pnpm -C ./packages/seeds run seed:test",
        "seed:prod": "pnpm -C ./packages/seeds run seed:prod",
        "seed:emulate": "pnpm -C ./packages/seeds run seed:emulate",
        "seed:congress": "pnpm -C ./packages/seeds run seed:congress",
        "BALTIMORE": "",
        "seed:dev:baltimore": "pnpm -C ./packages/seeds run seed:dev -- baltimore-maryland-united_states",
        "seed:prod:baltimore": "pnpm -C ./packages/seeds run seed:prod -- baltimore-maryland-united_states",
        "seed:emulate:baltimore": "pnpm -C ./packages/seeds run seed:emulate -- baltimore-maryland-united_states",
        "WASHGINGTON DC": "",
        "seed:dev:dc": "pnpm -C ./packages/seeds run seed:dev -- washington-district_of_columbia-united_states",
        "seed:prod:dc": "pnpm -C ./packages/seeds run seed:prod -- washington-district_of_columbia-united_states",
        "seed:emulate:dc": "pnpm -C ./packages/seeds run seed:emulate -- washington-district_of_columbia-united_states",
        "LOS ANGELES CA": "",
        "seed:dev:la": "pnpm -C ./packages/seeds run seed:dev -- los_angeles-california-united_states",
        "seed:prod:la": "pnpm -C ./packages/seeds run seed:prod -- los_angeles-california-united_states",
        "seed:emulate:la": "pnpm -C ./packages/seeds run seed:emulate -- los_angeles-california-united_states",
        "lint": "npx eslint -c packages/webapp/.eslintrc.js --ext ts,tsx '**/*'"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.27.1",
        "@typescript-eslint/parser": "^5.27.1",
        "autoprefixer": "10.4.5",
        "eslint": "^8.17.0",
        "eslint-import-resolver-typescript": "^2.7.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-only-warn": "^1.0.3",
        "npm-check-updates": "^13.1.3",
        "prettier": "^2.7.1",
        "typescript": "^4.7.3"
    }
}
